<div class="card shadow mb-4">
    <div class="card-header py-3">
        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#AddModal">
            <i class="fas fa-plus"></i>
        </button>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Nama</th>
                        <th>Nomor Rekening</th>
                        <th>Nama Bank</th>
                        <th>Image</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    
                    <% data.forEach((element,key) =>{%>
                    <tr>
                        <td><%=key %></td>
                        <td><%=element.name %></td>
                        <td><%=element.nomorRekening %></td>
                        <td><%=element.nameBank %></td>
                        <td>
                            <img src="/<%=element.imageUrl %>" alt="<%=element.nameBank %>"
                                style="object-fit: fill; width: 100px;">
                        </td>
                        <td>
                            <form action="/admin/bank/<%=element._id %>?_method=DELETE" method="POST">
                                <a href="javascript:void()" type="button"
                                    class="btn btn-warning btn-circle btn-sm button-update" data-id="<%= element._id %>"
                                    data-name="<%= element.name %>" data-bank="<%= element.nameBank %>"
                                    data-norek="<%= element.nomorRekening %>"><i class="fas fa-edit"></i></a>
                                <button type="submit" class="btn btn-danger btn-circle btn-sm btn-delete"
                                    data-id="<%=element._id %>"><i class="fas fa-trash"></i></button>
                            </form>
                        </td>
                    </tr>
                    <%})%>

                </tbody>
            </table>
        </div>
    </div>
</div>
